def fibonacci(n):
    if n <= 0:
        print("Input should be a positive integer.")
    elif n == 1:
        return 0
    elif n == 2:
        return 1
    else:
        return fibonacci(n-1) + fibonacci(n-2)

n = int(input("Enter the number of terms in the series: "))
print("Fibonacci Series:")
for i in range(1, n+1):
    print(fibonacci(i))
